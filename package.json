{
  "name": "bootstrap-boilerplate",
  "version": "0.0.1",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulptask-dev-server": "https://github.com/MasatoMakino/gulptask-dev-server.git",
    "gulptask-ejs": "https://github.com/MasatoMakino/gulptask-ejs.git",
    "gulptask-imagemin": "https://github.com/MasatoMakino/gulptask-imagemin.git",
    "gulptask-sass": "https://github.com/MasatoMakino/gulptask-sass.git",
    "gulptask-webpack": "https://github.com/MasatoMakino/gulptask-webpack.git",
    "husky": "^3.0.1",
    "jquery": "^3.4.1",
    "lazysizes": "^5.1.0",
    "make-dir": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.0.0",
    "popper.js": "^1.15.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "scripts": {
    "start:dev": "npm-run-all -s build:dev watch",
    "watch:server": "gulp server",
    "watch": "npm-run-all -p watch:*",
    "watch:gulp": "gulp watchTasks",
    "build:dev": "npm-run-all -s clean publish:build",
    "build:release": "npm-run-all -s clean publish:release",
    "clean": "gulp clean",
    "sass": "gulp sass",
    "ejs": "gulp ejs",
    "javascript:dev": "gulp bundleDevelopment",
    "javascript:production": "gulp bundleProduction",
    "image": "gulp images",
    "publish:build": "npm-run-all -p sass ejs javascript:dev image",
    "publish:release": "npm-run-all -p sass ejs javascript:production image"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": "git@bitbucket.org:masato_makino_bitbucket/bootstrap-boilerplate.git",
