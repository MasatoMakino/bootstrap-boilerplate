{
  "name": "bootstrap-boilerplate",
  "version": "0.2.1",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@masatomakino/gulptask-dev-server": "^0.4.1",
    "@masatomakino/gulptask-ejs": "^0.3.2",
    "@masatomakino/gulptask-imagemin": "^0.3.2",
    "@masatomakino/gulptask-revision": "^0.3.2",
    "@popperjs/core": "^2.11.5",
    "babel-loader": "^8.2.5",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.1.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "lazysizes": "^5.3.2",
    "prettier": "^2.7.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "overrides": {
    "glob-parent": "^5.1.2"
  },
  "scripts": {
    "start:dev": "npm run build:dev && concurrently 'gulp start_dev' 'npm:server' 'npm:watch-*' ",
    "build:dev": "gulp build_dev && concurrently 'npm:build-*'",
    "build:release": "npm run clean && concurrently 'npm:release-*' && gulp build_production",
    "//private script": "このラインはコメント行です。これ以下のスクリプトはbuildコマンド内で呼び出されます。変更しないでください。",
    "clean": "rimraf ./dist/",
    "build-EJS": "npx @masatomakino/gulptask-ejs --srcDir ./src --distDir ./dist --componentPatterns '**/_*.ejs'",
    "release-EJS": "npm run build-EJS",
    "watch-EJS": "npm run build-EJS -- -W",
    "build-sass": "sass --load-path node_modules/bootstrap/scss ./src/scss/style.scss ./dist/css/style.css",
    "release-sass": "npm run build-sass -- --style compressed",
    "watch-sass": "npm run build-sass -- --watch",
    "JS": "webpack",
    "build-JS": "npm run JS -- --mode development",
    "release-JS": "npm run JS -- --mode production --progress --color",
    "build-image": "npx @masatomakino/gulptask-imagemin --srcDir ./src/img --distDir ./dist --scales '',1.0",
    "release-image": "npm run build-image",
    "watch-image": "npm run build-image -- -W",
    "server": "npx @masatomakino/gulptask-dev-server -b ./dist"
  },
  "repository": "git@github.com:MasatoMakino/bootstrap-boilerplate.git",
  "author": "MasatoMakino",
  "license": "MIT"
}
